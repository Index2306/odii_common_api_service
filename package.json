    "serve": "node ./dist/index.js",
    "serve:worker": "node ./dist/product-worker.js",
    "dev": "node_modules/.bin/babel-node src/index.js",
    "dev:worker-product": "nodemon --exec node_modules/.bin/babel-node src/product-worker.js",
    "dev:worker-common": "nodemon --exec node_modules/.bin/babel-node src/common-worker.js",
